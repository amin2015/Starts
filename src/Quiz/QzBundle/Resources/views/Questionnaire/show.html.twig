{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Questionnaire</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Languageid</th>
                <td>{{ entity.languageId.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Welcomemessage</th>
                <td>{{ entity.welcomeMessage }}</td>
            </tr>
            <tr>
                <th>Attachement</th>
                <td>{{ entity.attachement }}</td>
            </tr>
            <tr>
                <th>Isgenerated</th>
                <td>{{ entity.isGenerated }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Confirmationsms</th>
                <td>{{ entity.confirmationSms }}</td>
            </tr>
            <tr>
                <th>Finalmessage</th>
                <td>{{ entity.finalMessage }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('questionnaire') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('questionnaire_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
